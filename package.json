{
  "name": "clasp-react-sample",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "clean": "rm -rf dist && npm run clean --workspaces",
    "typecheck": "npm run typecheck -w frontend",
    "build": "npm run build --workspaces && mkdir -p dist && cp frontend/dist/* dist/ && cp backend/dist/* dist/ && cp appsscript.json dist/appsscript.json",
    "dev": "npm run dev -w frontend",
    "setup-clasp": "node setup-clasp.mjs",
    "push": "npx clasp push",
    "deploy": "npx clasp deploy",
    "release": "npm run build && npm run push && npm run deploy"
  },
  "devDependencies": {
    "@google/clasp": "^3.1.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}
